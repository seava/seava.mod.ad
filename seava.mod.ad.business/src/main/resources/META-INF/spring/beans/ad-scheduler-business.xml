<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
		<bean id="JobContext" scope="singleton" lazy-init="true"
			class="seava.ad.business.ext.scheduler.service.JobContext_Service"/>				
	
		<bean id="JobContextParam" scope="singleton" lazy-init="true"
			class="seava.ad.business.ext.scheduler.service.JobContextParam_Service"/>				
	
		<bean id="JobTimer" scope="singleton" lazy-init="true"
			class="seava.ad.business.ext.scheduler.service.JobTimer_Service"/>				
	
		<bean id="JobLog" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.JobLog_Service"/>				
	
		<bean id="JobLogMessage" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.JobLogMessage_Service"/>				
	
		<bean id="QuartzBlobTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzBlobTrigger_Service"/>				
	
		<bean id="QuartzCalendar" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzCalendar_Service"/>				
	
		<bean id="QuartzCronTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzCronTrigger_Service"/>				
	
		<bean id="QuartzFiredTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzFiredTrigger_Service"/>				
	
		<bean id="QuartzJobDetail" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzJobDetail_Service"/>				
	
		<bean id="QuartzLock" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzLock_Service"/>				
	
		<bean id="QuartzPausedTriggerGroup" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzPausedTriggerGroup_Service"/>				
	
		<bean id="QuartzSchedulerState" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzSchedulerState_Service"/>				
	
		<bean id="QuartzSimpleTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzSimpleTrigger_Service"/>				
	
		<bean id="QuartzSimpropTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzSimpropTrigger_Service"/>				
	
		<bean id="QuartzTrigger" scope="singleton" lazy-init="true"
			class="seava.ad.business.impl.scheduler.QuartzTrigger_Service"/>				
