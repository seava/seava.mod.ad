<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="nan21" id="TBL-AD_JOBLOG_MSG">
		<createTable tableName="AD_JOBLOG_MSG">
			<column name="ACTIVE" type="number(1)">
				<constraints nullable="false"/>
			</column>
			<column name="CLIENTID" type="varchar2(64)">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDAT" type="DATE">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDBY" type="varchar2(32)">
				<constraints nullable="false"/>
			</column>
			<column name="ID" type="varchar2(64)">
				<constraints nullable="false"/>
			</column>
			<column name="MESSAGE" type="varchar2(4000)"/>
			<column name="MESSAGETYPE" type="varchar2(16)">
				<constraints nullable="false"/>
			</column>
			<column name="MODIFIEDAT" type="DATE">
				<constraints nullable="false"/>
			</column>
			<column name="MODIFIEDBY" type="varchar2(32)">
				<constraints nullable="false"/>
			</column>
			<column name="NOTES" type="varchar2(4000)"/>
			<column name="REFID" type="varchar2(64)">
				<constraints nullable="false"/>
			</column>
			<column name="VERSION" type="number(10)"/>
			<column name="JOBLOG_ID" type="varchar2(64)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="nan21" id="PK-AD_JOBLOG_MSG">
		<addPrimaryKey 
			constraintName="PK_AD_JOBLOG_MSG"
			tableName="AD_JOBLOG_MSG"
			columnNames="ID"/>
	</changeSet>

</databaseChangeLog>
