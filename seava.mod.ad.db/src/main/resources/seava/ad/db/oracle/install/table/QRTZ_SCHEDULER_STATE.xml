<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="nan21" id="TBL-QRTZ_SCHEDULER_STATE">
		<createTable tableName="QRTZ_SCHEDULER_STATE">
			<column name="CHECKIN_INTERVAL" type="number(13)"/>
			<column name="INSTANCE_NAME" type="varchar2(255)">
				<constraints nullable="false"/>
			</column>
			<column name="LAST_CHECKIN_TIME" type="number(13)"/>
			<column name="SCHED_NAME" type="varchar2(255)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="nan21" id="PK-QRTZ_SCHEDULER_STATE">
		<addPrimaryKey 
			constraintName="PK_QRTZ_SCHEDULER_STATE"
			tableName="QRTZ_SCHEDULER_STATE"
			columnNames="SCHED_NAME,INSTANCE_NAME"/>
	</changeSet>

</databaseChangeLog>
