<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="seava" id="TBL-QRTZ_PAUSED_TRIGGER_GRPS">
		<createTable tableName="QRTZ_PAUSED_TRIGGER_GRPS">
			<column name="SCHED_NAME" type="varchar2(255)">
				<constraints nullable="false"/>
			</column>
			<column name="TRIGGER_GROUP" type="varchar2(255)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="seava" id="PK-QRTZ_PAUSED_TRIGGER_GRPS">
		<addPrimaryKey 
			constraintName="PK_QRTZ_PAUSED_TRIGGER_GRPS"
			tableName="QRTZ_PAUSED_TRIGGER_GRPS"
			columnNames="SCHED_NAME,TRIGGER_GROUP"/>
	</changeSet>

</databaseChangeLog>
