<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="amathe" id="TBL">
		<createTable tableName="QRTZ_CALENDARS">
	<column name="CALENDAR" type="blob">
		<constraints nullable="false"/>
	</column>
	<column name="CALENDAR_NAME" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	<column name="SCHED_NAME" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	</createTable>
	</changeSet>

	<changeSet author="amathe" id="PK">
		<addPrimaryKey 
			constraintName="PK_QRTZ_CALENDARS"
			tableName="QRTZ_CALENDARS"
			columnNames="SCHED_NAME,CALENDAR_NAME"/>
	</changeSet>

</databaseChangeLog>
