<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="amathe" id="TBL">
		<createTable tableName="SYS_CLIENT">
	<column name="ACTIVE" type="number(1)">
		<constraints nullable="false"/>
	</column>
	<column name="ADMINROLE" type="varchar2(32)"/>
	<column name="CODE" type="varchar2(64)">
		<constraints nullable="false"/>
	</column>
	<column name="CREATEDAT" type="DATE">
		<constraints nullable="false"/>
	</column>
	<column name="CREATEDBY" type="varchar2(32)">
		<constraints nullable="false"/>
	</column>
	<column name="DESCRIPTION" type="varchar2(400)"/>
	<column name="EXPORTPATH" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	<column name="ID" type="varchar2(64)">
		<constraints nullable="false"/>
	</column>
	<column name="IMPORTPATH" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	<column name="MODIFIEDAT" type="DATE">
		<constraints nullable="false"/>
	</column>
	<column name="MODIFIEDBY" type="varchar2(32)">
		<constraints nullable="false"/>
	</column>
	<column name="NAME" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	<column name="NOTES" type="varchar2(4000)"/>
	<column name="REFID" type="varchar2(64)">
		<constraints nullable="false"/>
	</column>
	<column name="TEMPPATH" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	<column name="VERSION" type="number(10)"/>
	<column name="WORKSPACEPATH" type="varchar2(255)">
		<constraints nullable="false"/>
	</column>
	</createTable>
	</changeSet>

	<changeSet author="amathe" id="PK">
		<addPrimaryKey 
			constraintName="PK_SYS_CLIENT"
			tableName="SYS_CLIENT"
			columnNames="ID"/>
	</changeSet>

</databaseChangeLog>
